{
    "buildCommand": "chmod +x build.sh && ./build.sh",
    "devCommand": "npm run start",
    "installCommand": "npm install --legacy-peer-deps",
    "framework": "create-react-app",
    "outputDirectory": "build",
    "routes": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}